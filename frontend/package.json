{
  "name": "aitm-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite dev --port 41241 --host 127.0.0.1",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:smoke": "playwright test --grep=\"Smoke Tests\"",
    "test:e2e:projects": "playwright test --grep=\"Project Management\"",
    "test:e2e:analysis": "playwright test --grep=\"Threat Analysis\"",
    "test:e2e:api": "playwright test --grep=\"API Integration\""
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.28.0",
    "eslint-plugin-svelte": "^2.30.0",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2"
  },
  "dependencies": {
    "@playwright/test": "^1.54.2",
    "@tailwindcss/forms": "^0.5.6",
    "@types/js-cookie": "^3.0.6",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.0",
    "chart.js": "^4.5.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "d3": "^7.9.0",
    "date-fns": "^2.30.0",
    "fuse.js": "^7.0.0",
    "js-cookie": "^3.0.5",
    "lucide-svelte": "^0.294.0",
    "marked": "^9.1.6",
    "postcss": "^8.4.31",
    "prismjs": "^1.29.0",
    "svelte-chartjs": "^3.1.5",
    "svelte-loading-spinners": "^0.3.4",
    "tailwindcss": "^3.3.0"
  }
}
