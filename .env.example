# Database
DATABASE_URL=sqlite:///./aitm.db

# Ports (NEVER use 3000 or 8000)
BACKEND_PORT=38527
FRONTEND_PORT=41241

# Security
JWT_SECRET_KEY=your-super-secret-jwt-key-change-this-in-production
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30

# LLM Providers
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here
OLLAMA_BASE_URL=http://localhost:11434
LITELLM_API_KEY=your_litellm_api_key_here

# Default LLM Provider (options: openai, google, ollama, litellm)
DEFAULT_LLM_PROVIDER=google

# Monitoring
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT=aitm-development
LANGSMITH_TRACING=true

# Application Settings
ENVIRONMENT=development
LOG_LEVEL=INFO
CORS_ORIGINS=http://localhost:41241,http://127.0.0.1:41241

# MITRE ATT&CK
MITRE_ATTACK_VERSION=14.1
MITRE_ATTACK_DATA_URL=https://raw.githubusercontent.com/mitre/cti/master/enterprise-attack/enterprise-attack.json
